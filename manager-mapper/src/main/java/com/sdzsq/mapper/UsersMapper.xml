<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sdzsq.mapper.UsersMapper">

    <insert id="insertUser" parameterType="com.sdzsq.pojo.Users">
        insert into users(username,userage) values(#{userName},#{userAge})
    </insert>
    <select id="selectUserAll" resultType="com.sdzsq.pojo.Users">
        select * form users
    </select>
</mapper>